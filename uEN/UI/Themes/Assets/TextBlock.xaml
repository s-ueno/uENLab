<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:uEN.UI"
                    xmlns:attached="clr-namespace:uEN.UI.AttachedProperties"
                    >
    <FontFamily x:Key="SymbolFont">Segoe UI Symbol</FontFamily>
    <Style x:Key="SegoeUISymbolTextBlockKey" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="{StaticResource SymbolFont}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>
</ResourceDictionary>